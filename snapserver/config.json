{
  "name": "Snapserver",
  "version": "0.0.9",
  "slug": "snapserver",
  "description": "Snapcast server",
  "url": "https://github.com/uspike/hassio-addons/",
  "ingress": true,
  "ingress_entry": "index.html",
  "startup": "services",
  "arch": ["amd64", "i386", "armhf", "armv7", "aarch64"],
  "boot": "auto",
  "hassio_api": true,
  "hassio_role": "default",
  "host_dbus": true,
  "ports": {
    "1704/tcp": 1704,
    "1705/tcp": 1705
  },
  "ports_description": {
    "1704/tcp": "Snapcast server port",
    "1705/tcp": "Snapcast TCP control port (set empty to disable)"
  },
  "map": ["share:rw"],
  "options": {
    "stream": {
      "streams": [
      ]
    }
  },
  "schema": {
    "stream": {
      "streams": ["str"],
      "buffer": "int?",
      "codec": "str?",
      "send_to_muted": "str?",
      "sampleformat": "str?"
    }
  }
}
